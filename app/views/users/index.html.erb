<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<% @users.each do |user| %>
  <% if current_user.id != user.id %>
    <p><%= link_to user.username, user_path(user.id) %> 
    <%= link_to "Follow", followships_path(:following_id => user), :method => :post %></p>
  <% end %>
<% end %>